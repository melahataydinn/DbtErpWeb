        <style>
            /* Minimalist Dropdown */
            .minimal-dropdown {
                appearance: none;
                padding: 5px 10px;
                border: 1px solid #ced4da;
                background-color: #fff;
                border-radius: 5px;
                font-size: 0.9rem;
                color: #6c757d;
                outline: none;
                transition: border-color 0.3s ease;
            }

                .minimal-dropdown:focus {
                    border-color: #80bdff;
                    box-shadow: 0 0 0 0.2rem rgba(0, 123, 255, 0.25);
                }

            .search-wrapper {
                position: relative;
            }

            .search-btn {
                background: transparent;
                border: none;
                padding: 8px;
                cursor: pointer;
                display: flex;
                align-items: center;
                font-size: 1.25rem;
            }

            .search-input-wrapper {
                display: none;
                position: absolute;
                top: 100%;
                right: 0;
                width: 250px;
                transition: 0.3s ease;
            }

            .search-input {
                width: 100%;
                padding: 5px 10px;
                border: 1px solid #ced4da;
                border-radius: 5px;
                outline: none;
                transition: border-color 0.3s ease;
            }

                .search-input:focus {
                    border-color: #80bdff;
                    box-shadow: 0 0 0 0.2rem rgba(0, 123, 255, 0.25);
                }

            /* Search Input Hover Effect */
            .search-wrapper:hover .search-input-wrapper {
                display: block;
            }

            .search-btn:hover .mdi-magnify {
                color: #007bff;
            }

