                    <div class="d-inline-block ms-2 no-print mt-3">
                        <form asp-controller="home" asp-action="SelectDatabase" method="post">
                            <select id="databaseDropdown" name="databaseName" class="minimal-dropdown" onchange="this.form.submit()">
                                @* Önce varsayılan olarak "Select Database" *@
                                <option value="" disabled selected>Select Database</option>

                                @if (ViewBag.Databases != null && ViewBag.Databases.Count > 0)
                                {
                                    // Session'dan seçili olan veritabanını alıyoruz
                                    var selectedDatabase = HttpContextAccessor.HttpContext.Session.GetString("SelectedDatabase");

                                    foreach (var db in ViewBag.Databases as List<string>)
                                    {
                                        // Eğer session'daki veritabanı mevcut ise, ilgili seçeneği seçili yapıyoruz
                                        if (db == selectedDatabase)
                                        {
                                            <option value="@db" selected>@db</option>
                                        }
                                        else
                                        {
                                            <option value="@db">@db</option>
                                        }
                                    }
                                }
                                else
                                {
                                    <option value="" disabled>No Databases Available</option>
                                }
                            </select>
                        </form>
